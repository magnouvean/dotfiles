{
  "ssh_connections": [
    {
      "host": "127.0.0.1",
      "username": "root",
      "port": 2223,
      "projects": [
        {
          "paths": [
            "/workspace"
          ]
        }
      ]
    }
  ],
  "icon_theme": "Catppuccin Mocha",
  "lsp": {
    "omnisharp": {
      "binary": {
        "path": "/omnisharp/OmniSharp",
        "arguments": ["optional", "additional", "args", "-lsp"]
      }
    }
  },
  "vim_mode": true,
  "relative_line_numbers": true,
  "ui_font_family": "JetBrains Mono",
  "ui_font_size": 16,
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte (Blur)",
    "dark": "Catppuccin Espresso (Blur)"
  },
  "vim-mode": true,
  "format_on_save": "on",
  "auto_install_extensions": {
    "basher": true,
    "catppuccin-blur": true,
    "catppuccin-icons": true,
    "clojure": true,
    "csharp": true,
    "dockerfile": true,
    "docker-compose": true,
    "html": true,
    "ini": true,
    "sql": true,
    "toml": true,
    "xml": true
  }
}
